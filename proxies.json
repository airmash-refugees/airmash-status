{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "root": {
      "matchCondition": {
          "methods": [ "GET" ],
          "route": "/"
      },
      "responseOverrides": {
        "response.statusCode": "302",
        "response.headers.location": "/status"
      }
    }
  }
}
